@page
@model LeitorNfe.Pages.Nfe.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Carregar</h1>

<h4>Nota Fiscal</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="FileUpload" class="control-label">Selecione um arquivo</label>
                <input asp-for="FileUpload" type="file" class="form-control"/>
                <span asp-validation-for="FileUpload" class="text-danger"></span>
            </div>
            <div class="form-group" style="margin-top: 20px;">
                <input type="submit" value="Carregar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Voltar</a>
</div>

